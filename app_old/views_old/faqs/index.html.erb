<% content_for :head do %>
  <%= javascript_include_tag :defaults %>
<% end %>

<% hide_right %>

<h1><%= title "Frequently Asked Questions" %></h1>

<% if @faqs.length > 0 %>
  <% for faq in @faqs %>
    <%= render "faqs/faq", :faq => faq %>
  <% end %>
  <%= will_paginate @faqs %>
<% else %>
 <p>No FAQs to display.</p>
<% end %>

<% content_for :left do  %>
  <div class="left_nav">
    <%= render :partial => "tags" %>
  </div>
<% end %>
