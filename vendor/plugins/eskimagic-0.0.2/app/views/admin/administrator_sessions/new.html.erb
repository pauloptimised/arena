<% title "Admin Login" %>

<% form_for [:admin, @administrator_session] do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>

<%= link_to "I've forgotten my password", new_admin_password_reset_path %>


