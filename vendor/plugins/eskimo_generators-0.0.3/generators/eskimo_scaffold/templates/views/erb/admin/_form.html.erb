<%% form_for [:admin, @<%= singular_name %>], :html => {:multipart => true} do |f| %>
  <%%= f.error_messages %>
<%- for attribute in attributes -%>
  <p>
    <%%= f.label :<%= attribute.name %> %><br />
    <%%= f.<%= attribute.field_type %> :<%= attribute.name %> %>
  </p>
<%- end -%>
  <p><%%= f.submit "Submit" %></p>
<%% end %>
